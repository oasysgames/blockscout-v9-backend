# Example environment variables for running the services
# Set these values when running docker compose

# Proxy Configuration
FRONT_PROXY_PASS=http://host.docker.internal:3000/
BACK_PROXY_PASS=http://backend:4000

# Error Scanner Configuration  
S3_ERROR_LOGS_BUCKET=oasys-blockscout-error-logs-bucket
S3_LOG_PREFIX=blockscout/errors
AWS_REGION=ap-northeast-1
SCAN_INTERVAL=600

# Slack Configuration (Required for alerts)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK
SLACK_CHANNEL=#alert-oasys
SLACK_USERNAME=Error Scanner
 SLACK_ICON_EMOJI=:warning:

# AWS Credentials (if not using IAM roles)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=

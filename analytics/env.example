# Analytics Service Environment Variables Template
# Copy to .env file in analytics/ directory and update with actual values

# S3 Configuration
S3_ACCESS_LOG_BUCKET=l1-mainnet-alb-access-logs-d3rsyke1
S3_ACCESS_LOG_PREFIX=AWSLogs/428784263071/elasticloadbalancing/ap-northeast-1
S3_ANALYSIS_PREFIX=analysis/access-logs
S3_ERROR_LOGS_BUCKET=oasys-blockscout-error-logs-bucket

# AWS Configuration
AWS_REGION=ap-northeast-1
AWS_SKIP_AWS_CHECK=false

# Slack Notification Configuration
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/ACTUAL/WEBHOOK
SLACK_CHANNEL=#alert-oasys
SLACK_USERNAME=Access Log Analyzer
SLACK_ICON_EMOJI=:bar_chart:

# Note: Analyzer runs daily at 2AM and analyzes access logs for previous day
# Usage: docker compose -f analytics/docker-compose.yml up -d
